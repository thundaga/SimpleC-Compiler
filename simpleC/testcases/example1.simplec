int factorialrecursive(int x) {
  int result;
  result = 1;
  if (x < 0 || x == 0) result = 1;
  else result = x * factorialrecursive(x - 1);
  return result;
}

int mult(int left, int right) {
  return left * right;
}

int factorialiterative(int x) {
  int result;
  result = 1;
  while (x > 0) {
    result = mult(result, x);
    x = x - 1;
  }
  return result;
}

int main() {
  print factorialrecursive(4);
  print factorialiterative(4);
  return 0;
}
